      "import": "./react/dist/bento-react.core.module.js",
      "require": "./react/dist/bento-react.core.js"
    }
  },
  "files": [
    "dist/*",
    "preact/*",
    "react/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ampproject/amphtml.git",
    "directory": "src/bento/core"
  },
  "homepage": "https://github.com/ampproject/amphtml/tree/main/src/bento/core"
