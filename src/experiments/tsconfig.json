{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "downlevelIteration": true,
    "target": "es5",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "lib": ["dom", "esnext", "es6"],
    "paths": {
      "#core": ["../core"],
      "#core/*": ["../core/*"],
      // "#experiments": ["."],
      "#experiments/*": ["./*"]
    }
  },
  "include": ["story-ad-*/**.js", "**/*.d.ts"],
  "exclude": ["**/*.extern.js"]
}
