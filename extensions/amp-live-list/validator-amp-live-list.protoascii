    name: "data-max-items-per-page"
    mandatory: true
    value_regex: "\\d+"
  }
  attrs: {
    name: "data-poll-interval"
    value_regex: "\\d{5,}"
  }
  attrs: {
    name: "disabled"
    value: ""
  }
  attrs: {
    name: "sort"
    value: "ascending"
  }
  attr_lists: "mandatory-id-attr"
  reference_points: {
    tag_spec_name: "AMP-LIVE-LIST [update]"
    mandatory: true
    unique: true
  }
  reference_points: {
