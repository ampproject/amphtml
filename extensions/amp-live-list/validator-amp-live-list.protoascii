    mandatory: true
    unique: true
  }
  reference_points: {
    tag_spec_name: "AMP-LIVE-LIST [pagination]"
    unique: true
  }
  amp_layout: {
    supported_layouts: CONTAINER
    supported_layouts: FIXED_HEIGHT
  }
}
