|  <body>
|      <h2>Gfycat</h2>
|      <h3>Responsive layout</h3>
|      <amp-gfycat
|              data-gfyid="TautWhoppingCougar"
|              width="640"
|              height="360"
|              layout="responsive">
|      </amp-gfycat>
|
|      <h3>Fixed layout</h3>
|      <amp-gfycat
|              data-gfyid="BareSecondaryFlamingo"
|              width="225"
|              height="400">
|      </amp-gfycat>
|
|      <h3>Autoplaying video is paused when out of view</h3>
|      <amp-gfycat
|              data-gfyid="LeanMediocreBeardeddragon"
|              width="640"
|              height="360">
|      </amp-gfycat>
|
|      <h3>Autoplay disabled</h3>
|      <amp-gfycat
|              data-gfyid="LeanMediocreBeardeddragon"
|              width="640"
|              height="360"
|              noautoplay>
|      </amp-gfycat>
