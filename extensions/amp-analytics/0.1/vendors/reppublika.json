{
  "requests": {
    "host": "https://t5.mindtake.com",
    "basePrefix": "/tag/cid/",
    "baseSuffix": "Service=${service}&Category=${category}&Url=${sourceUrl}&Device=${device}&uid=${random}",
    "pageview": "${host}${basePrefix}${code}/track.gif?${baseSuffix}"
  },
  "transport": {
    "beacon": false,
    "xhrpost": false,
    "image": true
  }
}
