{
  "transport": {
    "useBody": true,
    "beacon": true
  },
  "requests": {
    "onelink": "https://amp.onelink.me/amp/v1/onelink?af_id=COOKIE(afUserId)",
    "event": "https://amp.appsflyer.com/amp/v1/events?site-id=${webAppId}"
  },
  "triggers": {
    "onelink": {
      "on": "visible",
      "request": "onelink",
      "extraUrlParams": {
        "amp-afUserId": "CLIENT_ID(amp-afUserId)",
        "amp-afRefUserId": "COOKIE(amp-afRefUserId)",
        "afWebUserId": "COOKIE(afUserId)"
      }
    },
    "load": {
      "on": "visible",
      "request": "event",
      "extraUrlParams": {
        "data": {
          "amp-afUserId": "CLIENT_ID(amp-afUserId)",
          "amp-afRefUserId": "COOKIE(amp-afRefUserId)",
          "webAppId": "${webAppId}",
          "eventType": "LOAD",
          "afWebUserId": "COOKIE(afUserId)"
        },
        "meta": {
          "userAgent": "${userAgent}",
          "externalReferrer": "${externalReferrer}",
          "documentReferrer": "${documentReferrer}",
          "viewer": "${viewer}",
          "ampGeo": "${ampGeo}",
          "pageViewId": "${pageViewId}",
          "pageViewId64": "${pageViewId64}",
          "timestamp": "${timestamp}",
          "sourceHost": "${sourceHost}"
        }
      }
    }
  },
  "linkers": {
    "enabled": true,
    "proxyOnly": false,
    "linker": {
      "ids": {
        "afRefUserId": "CLIENT_ID(amp-afUserId)"
      }
    }
  },
  "cookies": {
    "enabled": true,
    "amp-afRefUserId": {
      "value": "LINKER_PARAM(linker, afRefUserId)"
    }
  }
}
