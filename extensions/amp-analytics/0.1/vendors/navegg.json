{
  "requests": {
    "pageview": "https://amp.navdmp.com/amp?aid=${clientId(navegg_id)}&url=${canonicalUrl}&ref=${documentReferrer}&tit=${title}&lan=${browserLanguage}&acc=${account}&wst=${wst}&wct=${wct}&wla=${wla}&v=7"
  },
  "triggers": {
    "trackpageview": {
      "on": "visible",
      "request": "pageview"
    }
  },
  "transport": {
    "beacon": false,
    "xhrpost": false,
    "image": {
      "suppressWarnings": true
    }
  }
}
