{
  "requests": {
    "base": "https://${p}-config.sensic.net/tp?${page_params}&${client_params}",
    "page_params": "r=${r}&ua=${ua}&v=${v}&l=${l}",
    "client_params": "p=${p}&m=${m}",
    "event_params": "ut=${ut}&pr=${pr}&rn=${rn}&c=${c}&vt=${vt}",
    "audio-play": "${base}&${event_params}&ty=PL&sn=COUNTER(play_${pr})",
    "audio-stop": "${base}&${event_params}&ty=ST&sn=COUNTER(stop_${pr})",
    "heartbeat": "${base}${event_params}&ty=HB"
  },
  "vars": {
    "ty": "HB",
    "c": "default",
    "v": "0.0.1",
    "r": "${ampdocHost}",
    "ut": "${timestamp}",
    "ua": "${userAgent}",
    "l": "${browserLanguage}",
    "pr": "${pageViewId}${name}",
    "rn": "${requestCount}",
    "vt": "1",
    "sp": "-1",
    "st": "${ut}"
  },
  "transport": {
    "beacon": false,
    "xhrpost": false,
    "image": true
  }
}
