{
  "plugins": [
    "eslint-plugin"
  ],
  "extends": [
    "plugin:eslint-plugin/recommended"
  ],
  "rules": {
    "jsdoc/check-param-names": 0,
    "jsdoc/check-tag-names": 0,
    "jsdoc/check-types": 0,
    "jsdoc/require-param": 0,
    "jsdoc/require-param-name": 0,
    "jsdoc/require-param-type": 0,
    "jsdoc/require-returns-type": 0
  }
}
