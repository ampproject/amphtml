  }
  attr_lists: "common-link-attrs"
  spec_url: "https://amp.dev/documentation/guides-and-tutorials/learn/spec/amphtml#html-tags"
}
tags: {
  html_format: AMP
  html_format: AMP4ADS
  html_format: ACTIONS
  tag_name: "LINK"
