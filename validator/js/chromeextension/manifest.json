{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.3.1",
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_icon": {
      "19": "no-amp-128.png",
      "38": "no-amp-38.png"
    }
  },
  "background": {
    "service_worker": "worker.js"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "homepage_url": "https://amp.dev",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*/",
    "<all_urls>"
  ]
}