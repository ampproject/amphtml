  }
  attrs: {
    name: "amp-boilerplate"
    mandatory: true
    value: ""
    dispatch_key: true
  }
}
tags: {
  name: "style"
  mandatory_parent: "noscript"
  detail: "noscript > style : boilerplate"
  also_requires: "head > style : boilerplate"
  unique: true
  mandatory_alternatives: "noscript > style : boilerplate"
