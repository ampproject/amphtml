{
  "bracketSpacing": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "overrides": [
    {
      "files": "OWNERS",
      "options": {
        "parser": "json5",
        "bracketSpacing": true,
        "trailingComma": "none"
      }
    },
    {
      "files": [".eslintrc", ".prettierrc", ".renovaterc.json"],
      "options": {"parser": "json"}
    },
    {
      "files": [".codecov.yml", ".lando.yml", ".lgtm.yml", ".travis.yml"],
      "options": {"parser": "yaml"}
    }
  ]
}
