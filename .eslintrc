{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "forOf": false
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "semi": 2,
    "google-camelcase/google-camelcase": 2,
    "no-eval": 2,
    "curly": 2,
    "radix": 2,
    "no-debugger": 2,
    "no-alert": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-implied-eval": 2,
    "no-native-reassign": 2,
    "no-lone-blocks": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-warning-comments": [2, { "terms": ["do not submit"], "location": "anywhere" }],
    "wrap-iife": [2, "any"],
    "no-unused-expressions": 0,
    "no-iterator": 2,
    "no-redeclare": 2,
    "no-script-url": 2,
    "no-implicit-coercion": [2, { "boolean": false }],
    "no-useless-concat": 2,
    "no-self-compare": 2,
    "no-useless-call": 2,
    "no-div-regex": 2,
    "max-len": [2, 80, 4, { "ignoreComments": true, "ignoreUrls": false, "ignorePattern": "" }]
  }
}
